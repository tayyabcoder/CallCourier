{
  "name": "callcourier",
  "version": "1.0.0",
  "description": "CallCourier project with Tailwind CSS setup",
  "main": "index.js",
  "scripts": {
    "dev": "npm run watch:css",
    "build": "npm run build:css",
    "build:css": "tailwindcss -i ./wwwroot/css/input.css -o ./wwwroot/css/output.css --minify",
    "watch": "npm run watch:css",
    "watch:css": "tailwindcss -i ./wwwroot/css/input.css -o ./wwwroot/css/output.css --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "tailwind",
    "courier",
    "logistics",
    "aspnet",
    "frontend"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^3.4.13"
  }
}
